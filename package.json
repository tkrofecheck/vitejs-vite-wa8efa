    "data-predictive": "workspace:*",
    "data-retailers": "workspace:*",
    "data-tracking": "workspace:*",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "types": "workspace:*",
    "typescript": "^4.9.3",
    "ui": "workspace:*",
    "utils": "workspace:*",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^3.0.0"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@sveltejs/adapter-auto": "2.0.0",
